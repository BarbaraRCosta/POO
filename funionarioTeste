package prova;

public class funcionarioTeste {

	public static void main(String[] args) {
		
		
		historico tempo = new historico ("Empresa Generica", 24f);
		funcionario rh = new funcionario ("Paulo", tempo);
		
		System.out.println("Funcionario: " + rh.getNome() );
		System.out.println("Empresa: " + rh.getHistoricoTrabalho().getNomeDaEmpresa());
		System.out.println("Tempo de trabalho em anos:" + rh.getHistoricoTrabalho().getTempoDeTrabalhoEmMeses());
		System.out.println("O tempo de trabalho em anos e: " + tempo.tempoDeTrabalhoEmAnos());
	}

}
